  "type": "module",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "next typegen && eslint --fix --max-warnings=0",
    "fix:markdownlint": "markdownlint --dot --fix .",
    "fix:pnpm-dedupe": "pnpm dedupe",
    "fix:prettier": "prettier --write .",
    "postinstall": "husky",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "next typegen && eslint --max-warnings=0",
    "lint:markdownlint": "markdownlint --dot .",
    "lint:pnpm-dedupe": "pnpm dedupe --check",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "next typegen && tsc --project .",
    "start": "next start"
  },
  "lint-staged": {
    "**": [
      "suppress-exit-code eslint --fix",
      "suppress-exit-code markdownlint --fix",
      "suppress-exit-code prettier --write"
    ]
  },
  "prettier": "@kachkaev/prettier-config",
  "dependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@vercel/analytics": "1.6.1",
