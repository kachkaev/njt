{
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix .",
    "fix:markdownlint": "markdownlint --dot --fix .",
    "fix:prettier": "prettier --write .",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint .",
    "lint:markdownlint": "markdownlint --dot .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "postinstall": "husky install",
    "start": "next start"
  },
  "lint-staged": {
    "**": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@vercel/analytics": "1.0.1",
    "hosted-git-info": "6.1.1",
    "lru-cache": "9.1.2",
    "next": "13.4.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.11",
    "styled-normalize": "8.0.7",
    "url": "0.11.0"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "0.6.0",
    "@kachkaev/markdownlint-config": "0.5.0",
    "@next/eslint-plugin-next": "13.4.5",
    "@rushstack/eslint-patch": "1.3.2",
    "@tsconfig/esm": "1.0.3",
    "@tsconfig/next": "1.0.5",
    "@tsconfig/strictest": "2.0.1",
    "@types/hosted-git-info": "3.0.2",
    "@types/node": "18.16.18",
    "@types/react": "18.2.12",
    "@types/styled-components": "5.1.26",
    "eslint": "8.42.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "markdownlint-cli": "0.34.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0-alpha.11",
    "prettier-plugin-packagejson": "2.4.3",
    "prettier-plugin-sh": "0.12.8",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@4.0.0-rc.45"
}
