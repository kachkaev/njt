{
  "private": true,
  "license": "BSD-3-Clause",
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --dot --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint --dot \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "postinstall": "husky install",
    "start": "next start"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kachkaev/react-inline-js": "^0.4.0",
    "hosted-git-info": "^5.0.0",
    "lru-cache": "^7.7.3",
    "next": "^12.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "styled-components": "^5.3.5",
    "styled-normalize": "^8.0.7",
    "url": "^0.11.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.4.5",
    "@kachkaev/markdownlint-config": "^0.3.0",
    "@kachkaev/prettier-config": "^1.3.1",
    "@next/eslint-plugin-next": "^12.1.4",
    "@rushstack/eslint-patch": "^1.1.1",
    "@types/hosted-git-info": "^3.0.2",
    "@types/lru-cache": "^7.6.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/styled-components": "^5.1.24",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "markdownlint-cli": "^0.31.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "packageManager": "yarn@3.2.0"
}
