{
  "private": true,
  "license": "BSD-3-Clause",
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "prepare": "husky install",
    "start": "next start"
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@kachkaev/react-inline-js": "^0.2.0",
    "hosted-git-info": "^4.0.2",
    "lru-cache": "^6.0.0",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.7",
    "url": "^0.11.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.2.3",
    "@kachkaev/markdownlint-config": "^0.2.0",
    "@kachkaev/prettier-config": "^1.2.0",
    "@types/hosted-git-info": "^3.0.2",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "markdownlint-cli": "^0.28.1",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "typescript": "^4.4.3",
    "yarn-deduplicate": "^3.1.0"
  }
}
