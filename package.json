{
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "workspaces": [
    "cli"
  ],
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "npm-run-all --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix .",
    "fix:markdownlint": "markdownlint --dot --fix .",
    "fix:prettier": "prettier --write .",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint": "npm-run-all --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint . --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint --dot .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --project .",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "postinstall": "husky install",
    "start": "next start"
  },
  "lint-staged": {
    "**": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "hosted-git-info": "^6.0.0",
    "lru-cache": "^7.14.0",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "styled-normalize": "^8.0.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-react": "^0.4.5",
    "@kachkaev/markdownlint-config": "^0.3.0",
    "@kachkaev/prettier-config": "^1.3.1",
    "@next/eslint-plugin-next": "^12.3.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/hosted-git-info": "^3.0.2",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@4.0.0-rc.23"
}
