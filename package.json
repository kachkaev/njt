    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "markdownlint-cli": "0.47.0",
    "npm-run-all2": "8.0.4",
